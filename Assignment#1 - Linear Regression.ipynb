    "## 19. Use your own implementation of the Gradient Descent to find the intercept and coeficients of the linear regression model"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 41,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[[-4.19455654e+02]\n",
      " [ 2.57626654e+02]\n",
      " [ 3.37139068e+02]\n",
      " [ 4.84923958e+02]\n",
      " [-2.12149097e+02]\n",
      " [-2.08360934e+02]\n",
      " [ 1.12284358e+04]\n",
      " [-1.16489839e+04]\n",
      " [-5.24646597e+02]\n",
      " [-9.23943996e-01]\n",
      " [-3.13532176e+02]\n",
      " [ 4.16243135e+02]]\n"
     ]
    }
   ],
   "source": [
    "X_b = np.c_[np.ones((1338, 1)), X]\n",
    "\n",
    "alpha= 0.00001\n",
    "num_iterations = 1000000\n",
    "m=1338\n",
    "W = np.random.randn(12,1)\n",
    "\n",
    "\n",
    "y1=y.values\n",
    "\n",
    "y2=y1.reshape((1338,1))\n",
    "for iteration in range(num_iterations):\n",
    "    gradients = 1/m * X_b.T.dot(X_b.dot(W) - y2)\n",
    "    W = W - alpha * gradients\n",
    "print(W)\n",
    "\n",
    "\n",
    "\n"
   ]
